@model IEnumerable<Project_IM.Models.Place_name>

<div class="d-flex d-flex-center">
    <select name="list" id="list">
        <option selected value="全部影片">全部影片</option>
        @foreach (var item in Model)
        {
            <option value="@Html.DisplayFor(modelItem => item.地點ID)">@Html.DisplayFor(modelItem => item.地點名稱)</option>
        }
    </select>
    <button id="s1" class="btn btn-ct" style="margin-left: 5%;" @*onclick="select1()"*@> 搜尋 </button>
</div>