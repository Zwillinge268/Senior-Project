@model IEnumerable<Project_IM.Models.Place_name>

<select id="s2" name="list2">
    <option disabled selected>請選擇起點</option>
    @foreach (var item in Model)
    {
        if (ViewBag.ID == (item.地點ID).ToString())
        {
            <option value="@Html.DisplayFor(modelItem => item.地點ID)" selected>@Html.DisplayFor(modelItem => item.地點名稱)</option>
        }
        else
        {
            <option value="@Html.DisplayFor(modelItem => item.地點ID)">@Html.DisplayFor(modelItem => item.地點名稱)</option>
        }
    }
</select>

